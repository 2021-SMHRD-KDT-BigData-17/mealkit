<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="database.mapper">
	<select id="signUpCheckId" parameterType="domain.User">
		select id from tbl_user where id =#{id}
	</select>
	
	<insert id="signUp">
		insert into values(#{user_id},#{user_pw},#{user_name},#{user_nick},#{user_gender},#{user_addr},#{user_phone},#{user_point, jdbcType=INTEGER},NOW(),#{user_type})
	</insert>
	
	
	
	
	
	
</mapper>